{
  "collectCoverageFrom": [
    "<rootDir>/src/**/*.{js}"
  ],
  "globals": {
  },
  "coveragePathIgnorePatterns": [
  ],
  "setupFiles": [
  ],
  "testMatch": [
    "<rootDir>/src/test/unit/**/?(*.)(spec|test).js"
  ],
  "testEnvironment": "node",
  "testURL": "http://localhost",
  "transform": {
    "^.+\\.(js)$": "<rootDir>/node_modules/babel-jest"
  },
  "transformIgnorePatterns": [
    "[/\\\\]node_modules[/\\\\].+\\.(js)$"
  ],
  "moduleNameMapper": {
  },
  "coverageThreshold": {
    "global": {
      "branches": 80,
      "functions": 90,
      "lines": 90,
      "statements": 90
    }
  }
}
